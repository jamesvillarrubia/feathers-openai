{
  "github": {
    "release": true
  },
  "npm": {
    "ignoreVersion": true,
    "publish": false,
    "skipChecks": true
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": {
        "name": "conventionalcommits",
        "types": [
          {
            "type": "feat",
            "section": "Features"
          },
          {
            "type": "fix",
            "section": "Bug Fixes"
          }
        ]
      }
    }
  }
}
